[wsl2]

# Limits VM memory in WSL 2 up to 3GB
memory=5GB

# Makes the WSL 2 VM use two virtual processors
# processors=2

# Set swap file to other location (https://learn.microsoft.com/en-us/windows/wsl/wsl-config#main-wsl-settings)
# swapfile=D:\\WSL\\swap.vhdx

# See https://github.com/microsoft/WSL/issues/10216
WSL2_WESTON_SHELL_OVERRIDE=desktop-shell
guiApplications=false

[experimental]
autoMemoryReclaim=gradual
sparseVhd=true
